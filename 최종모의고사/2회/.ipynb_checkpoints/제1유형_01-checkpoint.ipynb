    "import pandas as pd\n",
    "\n",
    "df = pd.read_csv('./data/Carseats.csv')\n",
    "upper = df['Sales'].mean() + df['Sales'].std() * 1.5\n",
    "lower = df['Sales'].mean() - df['Sales'].std() * 1.5\n",
    "df1 = df[(df['Sales'] <= upper) & (df['Sales'] >= lower)]\n",
    "print(df1['Age'].std())"
